pipeline {
    agent any

    stages{
        stage('npm install') {
            steps{
                sh 'cd ./automatet_test & npm i'
                sh 'cd ./passwordencrypt & npm i '    
            }
        } 

        stage('build') {
            steps{
                sh 'cd ./passwordencrypt & npm run build-react & npm run build-windows'
            }
        } 
    }
}