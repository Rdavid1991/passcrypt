pipeline {
    agent any

    stages{
        stage('npm install') {
            steps{
                sh 'npm i ./automatet_test'
                sh 'npm i ./passwordencrypt'    
            }
        } 

        stage('build') {
            steps{
                sh 'cd ./passwordencrypt & npm run build-react & npm run build-windows'
            }
        } 
    }
}